{
  "name": "cir-pricing-management",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "CIR Pricing Management System - B2B Industrial Distributor Pricing Tool",
  "keywords": [
    "pricing",
    "b2b",
    "industrial",
    "management",
    "supabase"
  ],
  "author": "CIR Team",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=frontend",
    "build": "npm run build --workspace=frontend",
    "preview": "npm run preview --workspace=frontend",
    "lint": "npm run lint --workspace=frontend",
    "type-check": "npm run type-check --workspace=frontend",
    "install:all": "npm install",
    "clean": "npm run clean:deps && npm run clean:cache",
    "clean:deps": "rm -rf node_modules frontend/node_modules package-lock.json frontend/package-lock.json",
    "clean:cache": "npm cache clean --force",
    "postinstall": "npm run postinstall --workspace=frontend --if-present"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "repository": {
    "type": "git",
    "url": "private"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b"
}
